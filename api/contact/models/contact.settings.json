{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "name": "contact"
  },
  "options": {
    "increments": false,
    "timestamps": false
  },
  "attributes": {
    "role": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    }
  }
}
